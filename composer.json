{
    "name": "brunonatali/inotify",
    "description": "A react PHP implementation for inotify, an filesystem change monitor",
    "keywords": ["inotify", "react", "filesystem", "eventloop"],
    "license": "MIT",
	"authors": [
	  {
		"name": "Bruno Natali",
		"email": "b010010010n@gmail.com"
	  }
	],
    "require": {
			"php": ">=7",
			"react/event-loop": "^1.1",
			"evenement/evenement": "^3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9"
	},
	"autoload": {
		"psr-4": {
			"BrunoNatali\\Inotify\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"BrunoNatali\\Tests\\Inotify\\": "tests/"
		}
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
